datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id          String @id @default(uuid())
  username    String @unique
  password    String
  bggUsername String
  createdAt   DateTime @default(now())
  deletedAt   DateTime?
}
